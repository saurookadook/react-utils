{
    "name": "react-utils",
    "version": "0.0.0",
    "description": "A collection of utilities for use with React.",
    "engines": {
        "node": ">=20.0.0",
        "pnpm": ">=8.0.0"
    },
    "packageManager": "pnpm@8.15.1",
    "workspaces": [
        "packages/*"
    ],
    "type": "module",
    "main": "index.js",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "pkgs:prepublish": "lerna version --no-private",
        "pkgs:publish": "lerna publish from-package",
        "test": "jest",
        "test:w": "jest --watchAll",
        "test:cov": "jest --coverage"
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.28.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.1.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/jest": "^29.5.14",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@typescript-eslint/eslint-plugin": "^8.13.0",
        "@vitejs/plugin-react-swc": "^3.7.1",
        "eslint": "^9.13.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.14",
        "globals": "^15.11.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "lerna": "^8.1.9",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.2.5",
        "typescript": "~5.6.2",
        "typescript-eslint": "^8.13.0",
        "vite": "^5.4.10"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saurookadook/react-utils.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/saurookadook/react-utils/issues"
    },
    "homepage": "https://github.com/saurookadook/react-utils#readme"
}
